<div id="countryData">
  <h2>Download Interview Data</h2>
  <ul>
    <% countries.each do |country| %>
      <li><a href="/country_data/<%= country[:code] %>"><%= country[:name] %></li></a>
    <% end %>
  </ul>
</div>

<div id="pollList">
  <h2>View Doodle Poll</h2>
  <ul>
    <% polls.sort_by! { |p| p[:title] }.each do |poll| %>
      <li><a href="http://doodle.com/<%= poll[:id] %>"><%= poll[:title] %></a></li>
    <% end %>
  </ul>
</div>
